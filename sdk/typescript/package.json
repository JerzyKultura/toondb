{
  "name": "@toondb/client",
  "version": "1.0.0",
  "description": "Official TypeScript/JavaScript SDK for ToonDB",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "toon",
    "toondb",
    "database",
    "llm",
    "ai",
    "tokens",
    "optimization"
  ],
  "author": "ToonDB Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/toondb/toondb-ts"
  },
  "dependencies": {
    "axios": "^1.6.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0"
  }
}

